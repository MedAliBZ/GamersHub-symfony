            <div class="container">
                <div class="row cus-mar">
                    <div class="col-lg-12">
                        <div class="table-responsive mt-60">
                            <table class="table">
                                <thead>
                      
                                <tr>
                                    <th scope="col">Id</th>
                                    <th scope="col">action</th>
                                    <th scope="col"> model</th>
                                </tr>
                                </thead>
                                <tbody>
